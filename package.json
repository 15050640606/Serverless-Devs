{
  "name": "@serverless-devs/s",
  "version": "1.1.13",
  "description": "Serverless devs tool, serverless developer tool, supports Alibaba cloud, AWS, azure, baidu cloud, Huawei cloud, Google cloud and Tencent cloud.",
  "homepage": "https://www.serverless-devs.cn",
  "keywords": [
    "serverless",
    "alibaba",
    "tencent",
    "azure",
    "baidu",
    "huawei",
    "google",
    "function",
    "faas",
    "serverless-devs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/serverless-devs/serverless-devs"
  },
  "bugs": {
    "email": "service@serverlessfans.com"
  },
  "bin": {
    "s": "./bin/s"
  },
  "main": "",
  "typings": "",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "watch": "tsc -w -p compile.tsconfig.json",
    "test": "nyc mocha -r ts-node/register scripts/tests/**/*.ts",
    "lint": "eslint --ext .js,.ts,.tsx ",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ",
    "build": "tsc -p compile.tsconfig.json",
    "release": "rm -rf ./lib && tsc -p compile.tsconfig.json && npm publish",
    "release-ignore": "npm publish"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "npm run lint:fix",
      "git add ."
    ],
    "**/*.{css,html,js,json,md,yaml,yml}": [
      "npm run prettier:fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@serverless-devs/s-progress-bar": "^0.0.2",
    "adm-zip": "^0.4.16",
    "axios": ">=0.21.1",
    "colors": "^1.4.0",
    "commander": "^6.0.0",
    "compressing": "^1.5.1",
    "download": "^7.1.0",
    "extract-zip": "^2.0.1",
    "fast-glob": "^3.2.4",
    "follow-redirects": "^1.13.0",
    "fs-extra": "^8.1.0",
    "i18n": "^0.12.0",
    "inquirer": "^7.3.3",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "progress": "^2.0.3",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "single-line-log": "^1.1.2",
    "superagent": "^6.1.0",
    "superagent-proxy": "^2.0.0"
  },
  "devDependencies": {
    "@types/assert": "^1.5.1",
    "@types/chai": "^4.2.12",
    "@types/download": "^6.2.4",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^7.3.0",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.23",
    "@types/js-yaml": "^3.12.5",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^26.4.2",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4",
    "babel-eslint": "^10.1.0"
  }
}
