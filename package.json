{
  "name": "@serverless-devs/s",
  "version": "1.1.15",
  "description": "Serverless devs tool, serverless developer tool, supports Alibaba cloud, AWS, azure, baidu cloud, Huawei cloud, Google cloud and Tencent cloud.",
  "homepage": "https://www.serverless-devs.cn",
  "keywords": [
    "serverless",
    "alibaba",
    "tencent",
    "azure",
    "baidu",
    "huawei",
    "google",
    "function",
    "faas",
    "serverless-devs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/serverless-devs/serverless-devs"
  },
  "bugs": {
    "email": "service@serverlessfans.com"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "publish": "lerna publish",
    "link:local": "lerna link --force-local",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan -i src",
    "fix": "f2elint fix",
    "typecheck": "npx tsc -p tsconfig.json --noEmit"
  },
  "bin": {
    "s": "packages/cli/bin/s"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@types/i18n": "^0.12.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.12.6",
    "f2elint": "^0.4.4",
    "git-cz": "^4.7.6",
    "jest": "^26.4.0",
    "lerna": "^3.22.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
